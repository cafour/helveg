{
    "extends": "@tsconfig/svelte/tsconfig.json",
    "compilerOptions": {
      "target": "ES2022",
      "module": "ES2022",
      "baseUrl": "./",
      "types": [
        "svelte"
      ],
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "allowJs": true,
      "noImplicitAny": false,
      "isolatedModules": true,
      "noEmit": true,
      "paths": {
        "helveg": [
            "../../UI/App/dist/helveg.d.ts"
        ]
      }
    },
    "exclude": [
      "node_modules/**",
      "eng/**",
      "dist/**",
      "template/**"
    ]
  }
  