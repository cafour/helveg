{
    "compilerOptions": {
        "noImplicitAny": true,
        "strictFunctionTypes": true,
        "strict": true,
        "noImplicitThis": true,
        "noEmitOnError": false,
        "removeComments": false,
        "sourceMap": true,
        "inlineSources": true,
        "target": "ESNext",
        "module": "ESNext",
        "declaration": true,
        "moduleResolution": "node",
        "strictNullChecks": true,
        "lib": [
            "dom",
            "es2015.promise",
            "es5",
            "es6"
        ],
    },
    "exclude": [
        "obj/**",
        "Resources/Scripts/tests",
        "node_modules/**"
    ],
    "compileOnSave": false
}
