{
  "name": "@cafour/helveg",
  "type": "module",
  "description": "An extensible software visualization tool.",
  "types": "./dist/helveg.d.ts",
  "files": [
    "./dist/helveg.d.ts"
  ],
  "license": "BSD-3-Clause",
  "version": "0.11.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/helveg/helveg"
  },
  "homepage": "https://helveg.net",
  "scripts": {
    "raw": "tsc --noEmit -p ./tsconfig.json && node --loader @esbuild-kit/esm-loader ./eng/build.ts",
    "build:Debug": "npm run raw -- --debug",
    "build:Release": "npm run raw -- --release",
    "build": "npm run build:Debug",
    "watch": "npm run raw -- --watch",
    "types": "svelte-package -i ./src -o ./dist -t && del-cli dist/**/*.js && del-cli dist/**/*.svelte && api-extractor run -l",
    "serve": "concurrently \"npm run watch -- --useTemplate\" \"browser-sync start -w -s ../obj/esbuild --host 0.0.0.0\""
  },
  "dependencies": {
    "graphology": "^0.25.4",
    "graphology-layout": "^0.6.1",
    "graphology-layout-forceatlas2": "^0.10.1",
    "graphology-types": "^0.24.7",
    "graphology-utils": "^2.5.2",
    "sigma": "3.0.0-alpha3",
    "svelte": "^4.2.1"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.6.5",
    "@microsoft/api-extractor": "^7.38.0",
    "@sveltejs/package": "^2.2.2",
    "@swc-node/register": "^1.6.8",
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/svelte": "^5.0.2",
    "@types/d3-hierarchy": "^3.1.4",
    "@types/file-saver": "^2.0.5",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^20.8.0",
    "@types/yargs": "^17.0.26",
    "autoprefixer": "^10.4.16",
    "browser-sync": "^2.29.3",
    "concurrently": "^8.2.1",
    "d3-hierarchy": "^3.1.2",
    "del-cli": "^5.1.0",
    "esbuild": "^0.19.4",
    "esbuild-sass-plugin": "^2.15.0",
    "esbuild-svelte": "^0.8.0",
    "file-saver": "^2.0.5",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "postcss-preset-env": "^9.1.4",
    "resolve": "^1.22.6",
    "sass": "^1.68.0",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "ts-node": "^10.9.1",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "uniformcss": "^1.5.4",
    "yargs": "^17.7.2"
  }
}
