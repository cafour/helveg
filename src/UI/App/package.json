{
  "name": "helveg",
  "type": "module",
  "scripts": {
    "raw": "ts-node --project ./tsconfig.node.json ./build.ts",
    "build:Debug": "npm run raw -- --debug",
    "build:Release": "npm run raw -- --release",
    "build": "npm run build:Debug",
    "watch": "npm run raw -- --watch",
    "serve": "concurrently \"npm run watch\" \"browser-sync start -w -s ../obj/esbuild\""
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "^1.0.0",
    "@tsconfig/node16": "^1.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.15.3",
    "@types/postcss-import": "^14.0.0",
    "@types/yargs": "^17.0.22",
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.28.3",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.11",
    "esbuild-plugin-copy": "^2.1.0",
    "esbuild-sass-plugin": "^2.6.0",
    "esbuild-style-plugin": "^1.6.1",
    "esbuild-svelte": "^0.7.3",
    "graphology": "^0.25.1",
    "graphology-layout": "^0.6.1",
    "graphology-layout-forceatlas2": "^0.10.1",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "resolve": "^1.22.1",
    "sass": "^1.59.3",
    "sigma": "^2.4.0",
    "svelte": "^3.56.0",
    "svelte-check": "^3.1.2",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "yargs": "^17.7.1"
  }
}
