{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Helveg (helveg.html)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/CommandLine/bin/Debug/net6.0/helveg.dll",
      "args": [
        "--preset",
        "Docs",
        "--verbose",
        "--mode",
        "SingleFile",
        "--out-dir",
        "C:\\temp\\",
        "-n",
        "helveg"
      ],
      "cwd": "C:\\dev\\helveg\\",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "Helveg (kafe_diff.html)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/CommandLine/bin/Debug/net6.0/helveg.dll",
      "args": [
        "--preset",
        "Dev",
        "--verbose",
        "--mode",
        "SingleFile",
        "--out-dir",
        "C:\\temp\\",
        "--compare-to",
        "C:\\dev\\fi\\kafe-old",
        "-n",
        "kafe_diff"
      ],
      "cwd": "C:\\dev\\fi\\kafe-api",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "Playground (Debug)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Playground/bin/Debug/net6.0/Helveg.Playground.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Playground",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "type": "node",
      "request": "launch",
      "name": "UI",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "--loader",
        "ts-node/esm"
      ],
      "args": [
        "${workspaceFolder}/src/UI/App/eng/build.ts"
      ],
      "cwd": "${workspaceFolder}/src/UI/App",
      "env": {
        "TS_NODE_PROJECT": "${workspaceFolder}/src/UI/App/eng/tsconfig.json"
      }
    },
    {
      "name": "Docfx sample",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "docfx-template-build",
      "program": "dotnet",
      "args": [
        "docfx",
        "build"
      ],
      "cwd": "${workspaceFolder}/samples/Docfx",
      "console": "internalConsole",
      "stopAtEntry": false
    },
  ]
}
