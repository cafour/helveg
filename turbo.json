{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputMode": "new-only"
        },
        "rebuild": {
            "dependsOn": [
                "^rebuild"
            ],
            "outputMode": "new-only"
        },
        "git-version": {
            "cache": false
        },
        "schema": {
            "outputMode": "new-only"
        }
    }
}
